{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ActionSetOne",
            "Effect": "Allow",
            "Action": [
                "ec2:RebootInstances",
                "ec2:DeleteSnapshot",
                "ec2:StartInstances",
                "ec2:CreateTags",
                "ec2:CreateSnapshots",
                "ec2:CreateSnapshot",
                "ec2:StopInstances",
                "ec2:GetConsoleScreenshot"
            ],
            "Resource": [
                "arn:aws:ec2:*:*:instance/*",
                "arn:aws:ec2:*::snapshot/*",
                "arn:aws:ec2:*:*:volume/*"
            ]
        }
        
    ]
}